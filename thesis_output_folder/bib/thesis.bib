	doi = {10.3386/w8841},
	file = {Full Text PDF:C\:\\Users\\tdounias\\Zotero\\storage\\H2DB3U2L\\Bertrand et al. - 2002 - How Much Should We Trust Differences-in-Difference.pdf:application/pdf}
}

